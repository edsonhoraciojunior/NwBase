    "type": "library",
    "keywords": [
        "zf2", "zend", "framework", "naturalweb"
    ],
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/naturalweb/NwBase",
    "authors": [
        {
            "name": "Renato Moura",
            "email": "renato@naturalweb.com.br"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "NwBase": "src/"
        },
        "classmap": [
            "./Module.php"
